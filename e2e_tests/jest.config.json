{
    "moduleNameMapper": {
        "@root(.*)$": "<rootDir>/src/$1"
    },
    "clearMocks": true,
    "setupFiles": [
        "<rootDir>/jest.setup.js"
    ],
    "testRegex": [
        "__tests__",
        ".*.spec.js"
    ],
    "transform": {
        "^.+\\.jsx?$": "babel-jest",
        "^.+/es/^.+$": "babel-jest"
    },
    "transformIgnorePatterns": [
        "/node_modules/(?!react-virtualized).+\\.js$",
        "_utils/websocket.js"
    ]
}
